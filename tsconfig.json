{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"lib": [
			"es2020",
		],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "dist"
	},
	"include": [
		"src/**/*.ts"
	]
}
